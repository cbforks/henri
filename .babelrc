{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "last 2 versions",
          "safari 7"
        ]
      }
    }],
    "react"
  ],
  "plugins": [
    "transform-async-to-generator",
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-runtime",
    "add-module-exports"
    ]
}